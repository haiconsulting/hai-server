{
  "functions": {
    "source": ".",
    "runtime": "nodejs18"
  },
  "hosting": {
    "site": "hai-consulting",
    "public": "public",
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "app"
      }
    ]
  }
} 
